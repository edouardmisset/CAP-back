// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model Ascent {
  id        Int       @default(autoincrement()) @id
  routeName String    @db.VarChar(255)
  topoGrade String    @db.VarChar(255)
  date      DateTime  @db.Date
  crag      String    @db.VarChar(255)
  sector String @db.VarChar(255)
  geographicalRegion String @db.VarChar(255)
  comments String @db.VarChar(255)
  climber   String    @db.VarChar(255)
  routeOrBoulder String   @db.VarChar(255)
  numberOfTries Int      @db.Integer
  personalGrade String @db.VarChar(255)
  height String @db.VarChar(255)
  profile String @db.VarChar(255)
  holds String @db.VarChar(255)
  rating String @db.VarChar(255)
}

model User {
  id        Int       @default(autoincrement()) @id
  email     String    @unique
  name      String?   @db.VarChar(100)
  password  String    @db.VarChar(100)
}